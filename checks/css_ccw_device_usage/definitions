[check]
author = "Nageswara R Sastry <nasastry@in.ibm.com>"
component = "css/ccw"
repeat = "7d"

[deps]
sys_platform=s390 or sys_platform=s390x

[param ratio_limit]
default = "50"

[param device_print_limit]
default = "5"

[sysinfo lscss]
program = "/sbin/lscss"

[exception many_unused_devices]
severity = low
